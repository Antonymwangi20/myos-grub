#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
   int total = 0, i, x;
   if (x < 25) {
         if (x < 10) {
            int arr[] = {30-x};
            for (i = 0; i < len(arr); i++) {
                total += arr[i];
                printf("the values of x are: %d\n", arr[i]);
                printf("the total is: %d\n", total);
             }
         }
         
    }
    return 0;
}